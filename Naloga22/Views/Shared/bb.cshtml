
@model Naloga22.Models.Domovi
@{
    ViewData["Title"] = "Create Domovi";
}
<h2>Create</h2>
<h4>Domovi</h4>
<hr />
<form asp-action="bb" class="form-horizontal">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label class="control-label">Leto_zgradnje</label>
        <input id="Leto_zgradnje" asp-for="Leto_zgradnje" class="form-control" />
        <span asp-validation-for="Leto_zgradnje" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label class="control-label">Naslov</label>
        <input id="Naslov" asp-for="Naslov" class="form-control" />
        <span asp-validation-for="Naslov" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label class="control-label">Stevilka</label>
        <input id="Stevilka" asp-for="Stevilka" class="form-control" />
        <span asp-validation-for="Stevilka" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input id="Submit" type="submit" value="Submit" class="btn bg-primary" />

    </div>
</form>
